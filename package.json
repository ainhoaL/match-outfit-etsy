{
  "name": "match-outfit-etsy",
  "version": "1.0.0",
  "description": "Choose a colour and the tool finds items in etsy that have colours in a related palette for a matching outfit.",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --hot --inline",
    "postinstall": "typings install",
    "typings": "typings",
    "test": "set NODE_ENV=test && karma start karma.config.js",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,tests,karma-test-shim.js",
    "coverage:report": "istanbul report",
    "build": "webpack --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ainhoaL/match-outfit-etsy"
  },
  "license": "",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "angular2-in-memory-web-api": "0.0.20",
    "angular2-infinite-scroll": "0.3.1",
    "core-js": "^2.4.1",
    "nearest-color": "^0.4.0",
    "ngx-color-picker": "^4.3.1",
    "nice-color-palettes": "^2.0.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-angular": "0.0.6",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^0.2.3",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.4",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript-preprocessor": "^0.2.1",
    "karma-webpack": "1.8.0",
    "less": "2.7.1",
    "less-loader": "^2.2.2",
    "remap-istanbul": "^0.9.5",
    "source-map-loader": "^0.2.1",
    "spyon": "^0.5.0",
    "style-loader": "0.13.1",
    "ts-loader": "^0.8.0",
    "typescript": "2.1.0-dev.20161023",
    "typings": "1.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2"
  }
}
